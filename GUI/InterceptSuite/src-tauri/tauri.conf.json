{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "interceptsuite",
  "version": "1.0.1",
  "identifier": "com.interceptsuite.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build && node src-tauri/prepare-resources.js Release",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "interceptsuite",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "resources/*"
    ],
    "externalBin": [],
    "copyright": "Â© 2025 InterceptSuite",
    "category": "DeveloperTool",
    "shortDescription": "TLS Intercept Suite",
    "longDescription": "A powerful TLS interception and analysis tool for network security analysis",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "tsp": false,
      "wix": {
        "language": "en-US"
      },
      "nsis": {
        "installMode": "currentUser",
        "languages": ["English"]
      }
    },
    "linux": {

    },
    "macOS": {
      "minimumSystemVersion": "10.13"
    }
  }
}
